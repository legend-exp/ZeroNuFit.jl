{
    "debug":false,
    "partitions":["config/partitions_l200.json"],
    "events":    ["config/events_l200.json"],
    "output_path": "output/fit_l200/",
    "overwrite": true,
    "light_output": false,
    "bat_fit": {
        "nsteps": 1e5,
         "nchains": 4
    },
    "plot": { 
        "fit_and_data": true,
        "bandfit_and_data": false,
        "scheme":"blue", 
        "alpha": 0.3
    },
    "signal": { 
        "upper_bound":1000, 
        "prior": "sqrt"
     },
    "bkg": {
        "upper_bound":0.1, 
        "prior": "uniform",
        "correlated": {"mode": "none", "range": "none"}
    },
    "bkg_only": false,
    "nuisance": { 
        "energy_scale" : {
            "correlated":false,
            "fixed":     false
            },
         "efficiency" : {
            "correlated":true,
            "fixed": false
            }
    }
}

